{
  "expo": {
    "name": "Deliverr",
    "slug": "deliverr",
    "description": "deliveroo clone food pick-up application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/deliveroo-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/deliver-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}